<div class="container" data-news-per-page="{{newsNum}}">
    <h2>NEWS</h2>
    <!-- TODO: Paginator -->
    {{#each news}}
    <div class="news">
        <div class="date">{{created_time}}</div>
        <h3>Turniere / FS-Spiele (Nicht im DFB NET)</h3>
        <div class="news-content marginizer">
            <p>{{message}}</p>
        </div>
    </div>
    {{/each}}
</div>